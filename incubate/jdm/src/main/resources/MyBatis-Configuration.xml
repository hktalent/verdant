    	<typeAlias alias="jdm_site_flow" type="com.jsptpd.jdm.dao.mapper.Jdm_site_flow" />
	</typeAliases>
	<mappers>
        <mapper resource="com/jsptpd/jdm/dao/mapper/scp_traffic_data.xml"/>
        <mapper resource="com/jsptpd/jdm/dao/mapper/jdm_site_flow.xml"/>
    </mappers>
</configuration> 